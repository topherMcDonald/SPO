                <SourcePart></SourcePart><!-- Leave blank. -->
                <TargetPart>C157.013</TargetPart><!-- Part number being ordered. -->
                <Salesperson>originating salesperson</Salesperson><!-- The name of the Draper salesperson. -->
                <ContactEmail>originatingsalespersonusername@draperinc.com</ContactEmail><!-- Email address of Draper salesperson. -->
                <RequestedShipDate>
                        <Year>2016</Year>
                        <Month>07</Month>
                        <Day>07</Day>
                </RequestedShipDate>
                <CarrierName>UPS</CarrierName><!--leave blnak-- Expedited shipping might go here ... -->
        </POInfo>
        <MOInfo>
                <Configs></Configs><!-- Must include this node but leave it empty. -->
        </MOInfo>
</OrderLine>
