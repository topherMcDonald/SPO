<OrderLine>
        <LineInstructions>
                <LineInstruction/>
        </LineInstructions>
        <POInfo>
                <Status>New</Status><!-- Always New for small parts. -->
                <Number>10</Number><!-- Sequential, by tens, usually. Assigned dynamically by source program. -->
                <Qty>1</Qty><!-- Quantity ordered. -->
                <QtyUOM>PC</QtyUOM><!-- Just default to piece (PC) -->
